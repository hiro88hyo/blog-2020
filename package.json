    "eslint-plugin-react": "7.20.3",
    "eslint-watch": "7.0.0",
    "gh-pages": "3.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "lost": "^9.0.0",
    "postcss-pxtorem": "^6.1.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "stylelint": "13.6.1",
